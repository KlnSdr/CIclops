{
  "application": {
    "name": "ciclops",
    "version": "1.3-snapshot",
    "dbHost": "thot-container",
    "devMode": false,
    "runner": {
      "maxCount": 5,
      "checkInterval": 5,
      "timeout": 60
    }
  },
  "dobby": {
    "staticContent": {
      "directory": "public/out"
    },
    "session": {
      "age": 2,
      "store": "hades.session.HadesSessionStore"
    },
    "port": 8080,
    "threads": 10,
    "mode": "http",
    "logLevel": "WARN"
  },
  "hades": {
    "disableWelcomeMessage": true,
    "apidocs": {
      "enabled": true,
      "hideHadesRoutes": true
    },
    "login": {
      "maxAttempts": 5,
      "lockDuration": 300000,
      "tokenHeaderName": "Hades-Login-Token",
      "redirect": {
        "success": "/",
        "successAdmin": "/hades"
      }
    },
    "unauthorizedRedirectTarget": "/hades/login",
    "unauthorizedRedirectPaths": [
      "/",
      "/index.html",
      "/create",
      "/create/index.html",
      "/projectDetails",
      "/projectDetails/index.html"
    ]
  }
}